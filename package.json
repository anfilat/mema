{
  "name": "scrap-count",
  "version": "0.1.0",
  "description": "Scrap Count app",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\"",
    "updatedb": "node upDB.js",
    "update": "npm install && npm run client:install && npm run client:build && npm run updatedb"
  },
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.2.4",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.3.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.2"
  }
}
